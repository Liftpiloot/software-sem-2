@{
    Layout = null;
}
<!DOCTYPE html>
<head>
    <title>Home</title>
    <Link rel="stylesheet" href="~/css/home.css"/>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&display=swap" rel="stylesheet">
    <meta name="viewport" content="width=device-width,height=device-height,initial-scale=1.0"/>
</head>

<body>
<div class="iron-logo">
    <img src="~/images/iron_logo_new.png" alt="logo" width="100%"/>
</div>

<div class="workout-card">
    <div class="workout-select">
        <input type="checkbox"/>
    </div>
    <div class="workout-info">
        <div class="workout-logo">
            <img src="~/images/bench_wireframe.png" alt="logo"/>
        </div>
        <div class="workout">
            <h1>Bench-press</h1>
            <div class="selectors">
                <div class="selector-div">
                    <h1>Weight:</h1>
                    <select name="Weight" class="selector">
                        <option value="10">10</option>
                        <option value="20">20</option>
                        <option value="30">30</option>
                    </select>
                </div>
                <div class="selector-div">
                    <h1>Reps:</h1>
                    <select name="Reps" class="selector">
                        <option value="10">10</option>
                        <option value="20">20</option>
                        <option value="30">30</option>
                    </select>
                </div>
                <div class="selector-div">
                    <h1>Sets:</h1>
                    <select name="Sets" class="selector">
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                    </select>
                </div>
            </div>
        </div>
    </div>
</div>